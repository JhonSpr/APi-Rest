{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/animes(.*)",
      "dest": "https://api-animes-theta.vercel.app/api/v1/animes$1"
    },
    {
      "src": "/api/v1/recien-agregados(.*)",
      "dest": "https://api-animes-theta.vercel.app/api/v1/recien-agregados$1"
    },
    {
      "src": "/",
      "dest": "https://api-animes-theta.vercel.app/"
    }
  ]
}
