{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/animes(.*)",
      "dest": "http://localhost:3000/api/v1/animes$1"
    },
    {
      "src": "/api/v1/recien-agregados(.*)",
      "dest": "http://localhost:3000/api/v1/recien-agregados$1"
    },
    {
      "src": "/",
      "dest": "http://localhost:3000"
    }
  ]
}
