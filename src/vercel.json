{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/v1/animes",
      "dest": "http://localhost:3000/api/v1/animes"
    },
    {
      "src": "/api/v1/recien-agregados",
      "dest": "http://localhost:3000/api/v1/recien-agregados"
    },
    {
      "src": "/(.*)",
      "dest": "http://localhost:3000"
    }
  ]
}
